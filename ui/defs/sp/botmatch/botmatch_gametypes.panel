	text "Teamplay Modes"
	cvarTest "ui_menutab" showCVar { "0" }
}
/* Free For All Modes */
itemDef {
	name ffa1
	background MP_WIDEBUTTONBG style 1 type ITEM_TYPE_BUTTON textstyle 2 textscale 0.5 textalignx 32 textaligny 24 forecolor MP_TEXTCOLOR backcolor MP_BUTTONBGCOLOR visible 1 hexcolor 0 1 2 3	// color schemes
	text "Free For All"
	rect MENU_ITEM_COLUMN_1 MENU_ITEM_1 MENU_ITEM_WIDTH MENU_ITEM_HEIGHT
	cvarTest "ui_menutab" showCVar { "0" }
	action {
		play MP_SND_SELECT;
		setcvar ui_gametype 0;
		setcvar ui_menutab 1;
