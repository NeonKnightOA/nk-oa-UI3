
/* Gametypes tab */
/* -----------------------------------------------------------
TAB 0 - GAMETYPE MODES
----------------------------------------------------------- */
itemDef { /* Title: Free for all modes */
	name dmgamefield
	background MP_DIALOGBG style 1 type 0 textstyle 2 textscale 0.5 decoration textalignx 32 textaligny 24 border 1 bordercolor MP_BOX_BORDER backcolor MP_DIALOGBGCOLOR visible 1 hexcolor 0 1 2 3	// color schemes
	rect MENU_ITEM_TITLE_X1 MENU_ITEM_TITLE_Y MENU_ITEM_TITLE_WIDTH MENU_ITEM_TITLE_HEIGHT
	text "Free for All Modes"
	cvarTest "ui_menutab" showCVar { "0" }
}
itemDef { /* Title: Teamplay modes */
	name teamgamefield
	background MP_DIALOGBG style 1 type 0 textstyle 2 textscale 0.5 decoration textalignx 32 textaligny 24 border 1 bordercolor MP_BOX_BORDER backcolor MP_DIALOGBGCOLOR visible 1 hexcolor 0 1 2 3	// color schemes
